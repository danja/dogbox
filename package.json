{
  "name": "dogbox",
  "version": "1.0.0",
  "description": "Dog vocalisation synth",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "docs": "jsdoc -c jsdoc.json",
    "ghp": "webpack --mode production && docs/github-pages/deploy.sh",
    "rp": "repomix -c repomix.config.json ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danja/dogbox.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "src/js/app.js",
  "module": "src/js/app.js",
  "browser": "dist/js/app.bundle.js",
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/danja/dogbox/issues"
  },
  "homepage": "https://github.com/danja/dogbox#readme",
  "devDependencies": {
    "@babel/core": "^7.27.3",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-env": "^7.27.2",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jsdoc": "^4.0.4",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.31",
    "postcss-loader": "7.3.3",
    "prettier": "3.1.1",
    "repomix": "^0.3.7",
    "style-loader": "^3.3.3",
    "vitest": "^1.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "evb": "^0.7.2"
  }
}
